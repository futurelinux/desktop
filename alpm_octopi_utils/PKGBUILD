pkgname=alpm_octopi_utils
pkgver=1.0.2
pkgrel=1
pkgdesc="Alpm utils for Octopi"
url="https://octopiproject.wordpress.com/"
arch=('x86_64')
license=('GPL3')
makedepends=('vala' 'git')
source=("git+https://github.com/aarnt/${pkgname}.git#commit=bd9fcd5")
sha1sums=('SKIP')
         
build() {
   cd ${pkgname}
   make
}

package() {
   cd ${pkgname}
   make install   
}
